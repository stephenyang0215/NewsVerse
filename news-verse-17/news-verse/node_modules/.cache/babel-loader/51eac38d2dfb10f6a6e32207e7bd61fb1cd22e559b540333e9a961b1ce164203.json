dding: 10px;\n  font-size: 64px;\n  border-bottom: #978f8f solid 1px;\n  margin-bottom: 10px;\n}\n.categorier_img_container {\n  display: flex;\n  flex-direction: column;\n  justify-content: center;\n  align-items: center;\n  margin-bottom: 20px;\n}\n.footer {\n  height: 80px;\n  background-color: rgb(20, 30, 40);\n  display: flex;\n  align-items: center;\n  padding: 0 200px 0 200px;\n  justify-content: space-between;\n}\n.scroll_conainer {\n  width: 100%;\n  overflow: auto;\n  scrollbar-width: none;\n  -ms-overflow-style: none;\n}\n.scroll_conainer::-webkit-scrollbar {\n  display: none;\n}\n</style>\n"],"mappings":"AA2QA;AACA;;AAEA,MAAAA,IAAA,GAAAC,OAAA;AACA;EACAC,IAAA;EACAF,KAAA;IACA;MACAA,IAAA,EAAAA,IAAA;MACAG,UAAA,GACA,YACA,iBACA,WACA,UACA,WACA,UACA,aACA;MACAC,WAAA;MACA;MACAC,MAAA,GACA,OACA,OACA,OACA,OACA,OACA,OACA,OACA,OACA,OACA;IAEA;EACA;EACAC,UAAA;EACAC,QAAA;IACAC,OAAA,CAAAC,GAAA,MAAAT,IAAA;EACA;EACAU,OAAA;IACAC,UAAAC,CAAA;MACAJ,OAAA,CAAAC,GAAA;MACA,KAAAI,IAAA,MAAAR,MAAA,EAAAO,CAAA,QAAAP,MAAA,CAAAO,CAAA;IACA;EACA;AACA","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}