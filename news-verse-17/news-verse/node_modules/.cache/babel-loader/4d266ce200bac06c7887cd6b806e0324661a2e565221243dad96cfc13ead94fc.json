{"ast":null,"code":"const data = require(\"../assets/data.json\");\nexport default {\n  data() {\n    return {\n      pageTitle: this.$route.query.title,\n      data: data,\n      currentPage: 1,\n      itemsPerPage: 5,\n      isDown: [false, false, false, false, false]\n    };\n  },\n  methods: {\n    goBack() {\n      this.$router.back();\n    },\n    nextPage() {\n      if (this.currentPage < this.totalPages) {\n        this.currentPage++;\n      }\n    },\n    prevPage() {\n      if (this.currentPage > 1) {\n        this.currentPage--;\n      }\n    },\n    downClick(i) {\n      console.log(\"click\");\n      this.$set(this.isDown, i, !this.isDown[i]);\n    }\n  },\n  computed: {\n    totalPages() {\n      return Math.ceil(this.data.articles.L.length / this.itemsPerPage);\n    },\n    paginatedData() {\n      const start = (this.currentPage - 1) * this.itemsPerPage;\n      return this.data.articles.L.slice(start, start + this.itemsPerPage);\n    }\n  },\n  mounted() {\n    console.log(this.data.articles.L);\n  }\n};","map":{"version":3,"names":["data","require","pageTitle","$route","query","title","currentPage","itemsPerPage","isDown","methods","goBack","$router","back","nextPage","totalPages","prevPage","downClick","i","console","log","$set","computed","Math","ceil","articles","L","length","paginatedData","start","slice","mounted"],"sources":["src/views/AboutView.vue"],"sourcesContent":["<template>\n  <div class=\"about\">\n    <div class=\"top\">\n      <div style=\"display: flex; padding: 10px\" @click=\"goBack\">\n        <i class=\"el-icon-back\"></i>\n      </div>\n      <h1>{{ pageTitle }}</h1>\n    </div>\n    <div class=\"content\">\n      <div v-for=\"(item, i) in paginatedData\" :key=\"i\" class=\"itemContainer\">\n        <div class=\"textContainer\">\n          {{ item.M.title.S }}\n        </div>\n\n        <div\n          v-if=\"!isDown[i]\"\n          @click=\"downClick(i)\"\n          style=\"cursor: pointer; font-size: 32px\"\n        >\n          <i class=\"el-icon-arrow-down\"></i>\n        </div>\n        <div\n          v-if=\"isDown[i]\"\n          @click=\"downClick(i)\"\n          style=\"cursor: pointer; font-size: 32px\"\n        >\n          <i class=\"el-icon-arrow-up\"></i>\n        </div>\n        <!-- <div v-if=\"isDown[i]\" style=\"width: 390px\">\n          <el-descriptions :title=\"item.M.title.S\">\n            <el-descriptions-item label=\"description\">{{\n              item.M.description.S\n            }}</el-descriptions-item>\n          </el-descriptions>\n        </div> -->\n      </div>\n      <div class=\"pagination\">\n        <button @click=\"prevPage\" :disabled=\"currentPage === 1\">上一页</button>\n        <span>第 {{ currentPage }} 页</span>\n        <button @click=\"nextPage\" :disabled=\"currentPage === totalPages\">\n          下一页\n        </button>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nconst data = require(\"../assets/data.json\");\n\nexport default {\n  data() {\n    return {\n      pageTitle: this.$route.query.title,\n      data: data,\n      currentPage: 1,\n      itemsPerPage: 5,\n      isDown: [false, false, false, false, false],\n    };\n  },\n  methods: {\n    goBack() {\n      this.$router.back();\n    },\n    nextPage() {\n      if (this.currentPage < this.totalPages) {\n        this.currentPage++;\n      }\n    },\n    prevPage() {\n      if (this.currentPage > 1) {\n        this.currentPage--;\n      }\n    },\n    downClick(i) {\n      console.log(\"click\");\n      this.$set(this.isDown, i, !this.isDown[i]);\n    },\n  },\n  computed: {\n    totalPages() {\n      return Math.ceil(this.data.articles.L.length / this.itemsPerPage);\n    },\n    paginatedData() {\n      const start = (this.currentPage - 1) * this.itemsPerPage;\n      return this.data.articles.L.slice(start, start + this.itemsPerPage);\n    },\n  },\n  mounted() {\n    console.log(this.data.articles.L);\n  },\n};\n</script>\n\n<style scoped>\n.top {\n}\n.content {\n  padding: 