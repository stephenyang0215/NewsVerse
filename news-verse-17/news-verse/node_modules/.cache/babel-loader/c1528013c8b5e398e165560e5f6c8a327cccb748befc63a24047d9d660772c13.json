{"ast":null,"code":"const data = require(\"../assets/data.json\");\nexport default {\n  data() {\n    return {\n      pageTitle: this.$route.query.title,\n      data: data,\n      currentPage: 1,\n      itemsPerPage: 5,\n      isDown: [false, false, false, false, false],\n      linkList: {}\n    };\n  },\n  methods: {\n    goBack() {\n      this.$router.back();\n    },\n    nextPage() {\n      if (this.currentPage < this.totalPages) {\n        this.currentPage++;\n      }\n      this.isDown = [false, false, false, false, false];\n    },\n    prevPage() {\n      if (this.currentPage > 1) {\n        this.currentPage--;\n      }\n      this.isDown = [false, false, false, false, false];\n    },\n    downClick(i) {\n      console.log(\"click\");\n      this.$set(this.isDown, i, !this.isDown[i]);\n    },\n    getDomain(url) {\n      const start = url.indexOf(\"://\") + 3;\n      const end = url.indexOf(\"/\", start);\n      return end === -1 ? url.substring(start) : url.substring(start, end);\n    }\n  },\n  computed: {\n    totalPages() {\n      return Math.ceil(this.data.articles.L.length / this.itemsPerPage);\n    },\n    paginatedData() {\n      const start = (this.currentPage - 1) * this.itemsPerPage;\n      return this.data.articles.L.slice(start, start + this.itemsPerPage);\n    }\n  },\n  created() {\n    console.log(this.data.articles.L);\n  }\n};","map":{"version":3,"names":["data","require","pageTitle","$route","query","title","currentPage","itemsPerPage","isDown","linkList","methods","goBack","$router","back","nextPage","totalPages","prevPage","downClick","i","console","log","$set","getDomain","url","start","indexOf","end","substring","computed","Math","ceil","articles","L","length","paginatedData","slice","created"],"sources":["src/views/AboutView.vue"],"sourcesContent":["<template>\n  <div class=\"about\">\n    <div class=\"top\">\n      <div style=\"display: flex; padding: 10px\" @click=\"goBack\">\n        <i class=\"el-icon-back\"></i>\n      </div>\n      <h1>{{ pageTitle }}</h1>\n    </div>\n    <div class=\"content\">\n      <div v-for=\"(item, i) in paginatedData\" :key=\"i\">\n        <div class=\"itemContainer\">\n          <div class=\"textContainer\">\n            {{ item.M.title.S }}\n          </div>\n          <div\n            v-if=\"!isDown[i]\"\n            @click=\"downClick(i)\"\n            style=\"cursor: pointer; font-size: 32px\"\n          >\n            <i class=\"el-icon-arrow-down\"></i>\n          </div>\n          <div\n            v-if=\"isDown[i]\"\n            @click=\"downClick(i)\"\n            style=\"cursor: pointer; font-size: 32px\"\n          >\n            <i class=\"el-icon-arrow-up\"></i>\n          </div>\n        </div>\n        <div v-if=\"isDown[i]\" class=\"summary\">\n          <el-card>\n            <el-descriptions :title=\"item.M.title.S\" :column=\"1\">\n              <el-descriptions-item label=\"description\">{{\n                item.M.description.S\n              }}</el-descriptions-item>\n              <el-descriptions-item label=\"content\">{{\n                item.M.content.S\n              }}</el-descriptions-item>\n            </el-descriptions>\n            <div>\n              <ul class=\"ul-item\">\n                <li\n                  class=\"li-item\"\n                  v-for=\"(i, index) in item.M.news_sources\"\n                  :key=\"index\"\n                >\n                  {{ i.title }}\n                  <el-link\n                    :href=\"'https://' + getDomain(i.url)\"\n                    target=\"_blank\"\n                    type=\"primary\"\n                    >{{ getDomain(i.url) }}</el-link\n                  >\n                  {{ i.rank }}\n\n                  <el-tooltip\n                    class=\"item\"\n                    effect=\"dark\"\n                    content=\"Top Left 提示文字\"\n                    placement=\"top-start\"\n                  >\n                    <el-button\n                      icon=\"el-icon-warning-outline\"\n                      type=\"text\"\n                    ></el-button>\n                  </el-tooltip>\n                </li>\n              </ul>\n            </div>\n          </el-card>\n        </div>\n      </div>\n      <div class=\"pagination\">\n        <button @click=\"prevPage\" :disabled=\"currentPage === 1\">上一页</button>\n        <span>第 {{ currentPage }} 页</span>\n        <button @click=\"nextPage\" :disabled=\"currentPage === totalPages\">\n          下一页\n        </button>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nconst data = require(\"../assets/data.json\");\n\nexport default {\n  data() {\n    return {\n      pageTitle: this.$route.query.title,\n      data: data,\n      currentPage: 1,\n      itemsPerPage: 5,\n      isDown: [false, false, false, false, false],\n      linkList: {},\n    };\n  },\n  methods: {\n    goBack() {\n      this.$router.back();\n    },\n    nextPage() {\n      if (this.currentPage < this.totalPages) {\n        this.currentPage++;\n      }\n      this.isDown = [false, false, false, false, false];\n    },\n    prevPage() {\n      if (this.currentPage > 1) {\n        this.currentPage--;\n      }\n      this.isDown = [false, false, false, false, false];\n    },\n    downClick(i) {\n      console.log(\"click\");\n      this.$set(this.isDown, i, !this.isDown[i]);\n    },\n    getDomain(url) {\n      const start = url.indexOf(\"://\") + 3;\n      const end = url.indexOf(\"/\", start);\n      return end === -1 ? url.substring(start) : url.substring(start, end);\n    },\n  },\n  computed: {\n    totalPages() {\n      return Math.ceil(this.data.articles.L.length / this.itemsPerPage);\n    },\n    paginatedData() {\n      const start = (this.currentPage - 1) * this.itemsPerPage;\n      return this.data.articles.L.slice(start, start + this.itemsPerPage);\n    },\n  },\n  created() {\n    console.log(this.data.articles.L);\n  },\n};\n</script>\n\n<style scoped>\n.top {\n}\n.content {\n  padding: 100px 300px;\n}\n.content-container {\n  display: flex;\n  flex-direction: column;\n  justify-content: center;\n  align-items: center;\n  height: 200px;\n  background: #f1f1f1;\n  border: 1px solid #ccc;\n  padding: 20px;\n  text-align: center;\n}\n.pagination {\n  margin-top: 20px;\n}\nbutton {\n  margin: 0 5px;\n}\n.textContainer {\n  white-space: nowrap;\n  overflow: hidden;\n  text-overflow: ellipsis;\n  font-size: 32px;\n}\n.itemContainer {\n  margin: 40px;\n  padding: 20px;\n  display: flex;\n  justify-content: space-between;\n  align-items: center;\n  background: #ccc;\n}\n.summary {\n  margin: 40px;\n  padding: 20px;\n  display: flex;\n  justify-content: center;\n  align-items: center;\n}\n.ul-item {\n  display: flex;\n  flex-direction: column;\n  align-items: flex-start;\n}\n.li-item {\n}\n.item {\n  margin: 4px;\n}\n</style>\n"],"mappings":"AAoFA,MAAAA,IAAA,GAAAC,OAAA;AAEA;EACAD,KAAA;IACA;MACAE,SAAA,OAAAC,MAAA,CAAAC,KAAA,CAAAC,KAAA;MACAL,IAAA,EAAAA,IAAA;MACAM,WAAA;MACAC,YAAA;MACAC,MAAA;MACAC,QAAA;IACA;EACA;EACAC,OAAA;IACAC,OAAA;MACA,KAAAC,OAAA,CAAAC,IAAA;IACA;IACAC,SAAA;MACA,SAAAR,WAAA,QAAAS,UAAA;QACA,KAAAT,WAAA;MACA;MACA,KAAAE,MAAA;IACA;IACAQ,SAAA;MACA,SAAAV,WAAA;QACA,KAAAA,WAAA;MACA;MACA,KAAAE,MAAA;IACA;IACAS,UAAAC,CAAA;MACAC,OAAA,CAAAC,GAAA;MACA,KAAAC,IAAA,MAAAb,MAAA,EAAAU,CAAA,QAAAV,MAAA,CAAAU,CAAA;IACA;IACAI,UAAAC,GAAA;MACA,MAAAC,KAAA,GAAAD,GAAA,CAAAE,OAAA;MACA,MAAAC,GAAA,GAAAH,GAAA,CAAAE,OAAA,MAAAD,KAAA;MACA,OAAAE,GAAA,UAAAH,GAAA,CAAAI,SAAA,CAAAH,KAAA,IAAAD,GAAA,CAAAI,SAAA,CAAAH,KAAA,EAAAE,GAAA;IACA;EACA;EACAE,QAAA;IACAb,WAAA;MACA,OAAAc,IAAA,CAAAC,IAAA,MAAA9B,IAAA,CAAA+B,QAAA,CAAAC,CAAA,CAAAC,MAAA,QAAA1B,YAAA;IACA;IACA2B,cAAA;MACA,MAAAV,KAAA,SAAAlB,WAAA,aAAAC,YAAA;MACA,YAAAP,IAAA,CAAA+B,QAAA,CAAAC,CAAA,CAAAG,KAAA,CAAAX,KAAA,EAAAA,KAAA,QAAAjB,YAAA;IACA;EACA;EACA6B,QAAA;IACAjB,OAAA,CAAAC,GAAA,MAAApB,IAAA,CAAA+B,QAAA,CAAAC,CAAA;EACA;AACA","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}