{"ast":null,"code":"import { getweatherAPI } from \"@/api/index\";\nexport default {\n  data() {\n    return {\n      weatherData: {},\n      temperature: null,\n      description: \"\",\n      iconUrl: \"\",\n      tempMax: \"\",\n      tempMin: \"\",\n      weatherLoading: false\n    };\n  },\n  computed: {},\n  methods: {\n    async getweatherData() {\n      try {\n        const res = await getweatherAPI();\n        this.weatherData = res;\n        const iconCode = res.weather[0].icon;\n        this.temperature = res.main.temp;\n        this.description = res.weather[0].description;\n        this.iconUrl = `http://openweathermap.org/img/wn/${iconCode}@2x.png`;\n        this.tempMax = res.main.temp_max;\n        this.tempMin = res.main.temp_min;\n        console.log(\"weatherData\", this.weatherData);\n        this.weatherLoading = false;\n      } catch (error) {\n        console.error(error);\n      }\n    }\n  },\n  mounted() {\n    this.getweatherData();\n  }\n};","map":{"version":3,"names":["getweatherAPI","data","weatherData","temperature","description","iconUrl","tempMax","tempMin","weatherLoading","computed","methods","getweatherData","res","iconCode","weather","icon","main","temp","temp_max","temp_min","console","log","error","mounted"],"sources":["src/components/Weather.vue"],"sourcesContent":["<template>\r\n  <div class=\"time-container\">\r\n    <div v-if=\"timeLoading\">加载中...</div>\r\n    <div v-else class=\"weatherContent\">\r\n      <div class=\"weatherCity\">{{ weatherData.name }}</div>\r\n      <div class=\"weatherDetails\">\r\n        <img\r\n          :src=\"iconUrl\"\r\n          alt=\"Weather icon\"\r\n          width=\"80px\"\r\n          height=\"80px\"\r\n          v-if=\"iconUrl\"\r\n        />\r\n        <p>{{ temperature }}°C</p>\r\n        <div class=\"tempRange\">\r\n          <span>{{ tempMax }}°C</span>\r\n          <span>{{ tempMin }}°C</span>\r\n        </div>\r\n      </div>\r\n      <div class=\"tempDescription\">\r\n        <div