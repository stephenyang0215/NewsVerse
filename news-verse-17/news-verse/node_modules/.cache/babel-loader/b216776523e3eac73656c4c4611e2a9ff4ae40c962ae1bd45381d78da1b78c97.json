{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\n// const data = require(\"../assets/data.json\");\nconst data = require(\"../assets/final_output_1107.json\");\nexport default {\n  data() {\n    return {\n      pageTitle: this.$route.query.title,\n      tableData: [],\n      currentPage: 1,\n      itemsPerPage: 5,\n      isDown: [false, false, false, false, false],\n      linkList: {},\n      topKeywords: []\n    };\n  },\n  methods: {\n    goBack() {\n      this.$router.back();\n    },\n    nextPage() {\n      if (this.currentPage < this.totalPages) {\n        this.currentPage++;\n      }\n      this.isDown = [false, false, false, false, false];\n    },\n    prevPage() {\n      if (this.currentPage > 1) {\n        this.currentPage--;\n      }\n      this.isDown = [false, false, false, false, false];\n    },\n    downClick(i) {\n      this.$set(this.isDown, i, !this.isDown[i]);\n    },\n    getDomain(url) {\n      const start = url.indexOf(\"://\") + 3;\n      const end = url.indexOf(\"/\", start);\n      return end === -1 ? url.substring(start) : url.substring(start, end);\n    },\n    filterNewsByKeyword(data, keyword) {\n      const filteredNews = [];\n\n      // 遍历每个大分类\n      for (const category in data) {\n        if (data[category]?.item) {\n          // 遍历每个分类下的新闻数组\n          data[category].item.forEach(newsItem => {\n            const relevantSources = newsItem.news_sources.filter(source => source.keywords.includes(keyword));\n            if (relevantSources.length > 0) {\n              filteredNews.push({\n                topic: newsItem.topic,\n                summary: newsItem.summary,\n                news_sources: newsItem.news_sources\n              });\n            }\n          });\n        }\n      }\n      return filteredNews;\n    }\n  },\n  computed: {\n    totalPages() {\n      return Math.ceil(this.tableData.item.length / this.itemsPerPage);\n    },\n    paginatedData() {\n      const start = (this.currentPage - 1) * this.itemsPerPage;\n      return this.tableData.item.slice(start, start + this.itemsPerPage);\n    }\n  },\n  created() {\n    switch (this.$route.query.title) {\n      case \"Business\":\n        this.tableData = data.business;\n        break;\n      case \"Entertainment\":\n        this.tableData = data.entertainment;\n        break;\n      case \"General\":\n        this.tableData = data.general;\n        break;\n      case \"Health\":\n        this.tableData = data.health;\n        break;\n      case \"Science\":\n        this.tableData = data.science;\n        break;\n      case \"Sports\":\n        this.tableData = data.sports;\n        break;\n      case \"Technology\":\n        this.tableData = data.technology;\n        break;\n      default:\n        const filteredData = this.filterNewsByKeyword(data, this.$route.query.title);\n        this.tableData = filteredData;\n    }\n    console.log(\"tableData\", this.tableData);\n    // 获取所有 news_sources 的关键词\n    const getAllKeywords = data => {\n      const allKeywords = [];\n      Object.values(data).forEach(category => {\n        category.item.forEach(topic => {\n          topic.news_sources.forEach(news => {\n            allKeywords.push(...news.keywords);\n          });\n        });\n      });\n      return allKeywords;\n    };\n\n    // 统计关键词频率\n    const countKeywords = keywords => {\n      return keywords.reduce((countMap, keyword) => {\n        countMap[keyword] = (countMap[keyword] || 0) + 1;\n        return countMap;\n      }, {});\n    };\n\n    // 筛选出出现次数最多的前十个关键词\n    const getTopKeywords = (keywordCount, topN = 10) => {\n      return Object.entries(keywordCount).sort((a, b) => b[1] - a[1]) // 按出现次数从高到低排序\n      .slice(0, topN) // 取前 topN 个\n      .map(([keyword, count]) => ({\n        keyword,\n        count\n      }));\n    };\n\n    // 运行逻辑\n    const allKeywords = getAllKeywords(data);\n    const keywordCount = countKeywords(allKeywords);\n    const topKeywords = getTopKeywords(keywordCount);\n    this.topKeywords = topKeywords;\n    console.log(\"前十个出现次数最多的关键词：\", topKeywords);\n  }\n};","map":{"version":3,"names":["data","require","pageTitle","$route","query","title","tableData","currentPage","itemsPerPage","isDown","linkList","topKeywords","methods","goBack","$router","back","nextPage","totalPages","prevPage","downClick","i","$set","getDomain","url","start","indexOf","end","substring","filterNewsByKeyword","keyword","filteredNews","category","item","forEach","newsItem","relevantSources","news_sources","filter","source","keywords","includes","length","push","topic","summary","computed","Math","ceil","paginatedData","slice","created","business","entertainment","general","health","science","sports","technology","filteredData","console","log","getAllKeywords","allKeywords","Object","values","news","countKeywords","reduce","countMap","getTopKeywords","keywordCount","topN","entries","sort","a","b","map","count"],"sources":["src/views/AboutView.vue"],"sourcesContent":["<template>\n  <div class=\"about\">\n    <div class=\"top\">\n      <div style=\"display: flex; padding: 10px; width: 40px\" @click=\"goBack\">\n        <i class=\"el-icon-back\" style=\"color: #fff; cursor: pointer\"></i>\n      </div>\n      <h1 class=\"title\">{{ pageTitle }}</h1>\n    </div>\n    <div class=\"content\">\n      <el-card style=\"background: #ece8d9; width: 78%\">\n        <div>\n          <div v-for=\"(item, i) in paginatedData\" :key=\"i\">\n            <div class=\"itemContainer\">\n              <div class=\"textContainer\">\n                {{ item.topic }}\n              </div>\n              <div\n                v-if=\"!isDown[i]\"\n                @click=\"downClick(i)\"\n                style=\"cursor: pointer; font-size: 32px\"\n              >\n                <i class=\"el-icon-arrow-down\"></i>\n              </div>\n              <div\n                v-if=\"isDown[i]\"\n                @click=\"downClick(i)\"\n                style=\"cursor: pointer; font-size: 32px\"\n              >\n                <i class=\"el-icon-arrow-up\"></i>\n              </div>\n            </div>\n            <div v-if=\"isDown[i]\" class=\"summary\">\n              <el-card style=\"background: #b7e4c7\">\n                <el-descriptions :title=\"item.topic\" :column=\"1\">\n                  <el-descriptions-item label=\"summary\">{{\n                    item.summary\n                  }}</el-descriptions-item>\n                </el-descriptions>\n                <div>\n                  <ul class=\"ul-item\">\n                    <li\n                      class=\"li-item\"\n                      v-for=\"(i, index) in item.news_sources\"\n                      :key=\"index\"\n                    >\n                      {{ i.title }}\n                      <el-link\n                        :href=\"'https://' + getDomain(i.url)\"\n                        target=\"_blank\"\n                        type=\"primary\"\n                        >{{ getDomain(i.url) }}</el-link\n                      >\n                      keyword:\n                      <span\n                        v-for=\"(kitem, k) in i.keywords\"\n                        style=\"\n                          margin: 0px 5px 0px 5px;\n                          color: #409eff;\n                          border-bottom: 1px solid #409eff;\n                          cursor: pointer;\n                        \"\n                        :key=\"k\"\n                        >{{ kitem }}</span\n                      >\n                      Score :{{ i.bias }}\n                      <el-popover\n                        placement=\"top-start\"\n                        width=\"600\"\n                        trigger=\"hover\"\n                        content=\"Stay informed with concise summaries and objective bias ratings across business, entertainment, health, science, sports, and technology news. Each category is organized by topic for streamlined browsing, while bias is rated from 0-3 (left-leaning), 4-6 (neutral), and 7-10 (right-leaning). Access diverse perspectives and make informed decisions with ease.\"\n                      >\n                        <el-button slot=\"reference\" type=\"text\"\n                          ><i class=\"el-icon-warning-outline\"></i\n                        ></el-button>\n                      </el-popover>\n                    </li>\n                  </ul>\n                </div>\n              </el-card>\n            </div>\n          </div>\n        </div>\n\n        <div class=\"pagination\">\n          <button @click=\"prevPage\" :disabled=\"currentPage === 1\">\n            ← previous\n          </button>\n          <span>page {{ currentPage }} </span>\n          <button @click=\"nextPage\" :disabled=\"currentPage === totalPages\">\n            next →\n          </button>\n        </div>\n      </el-card>\n      <el-card style=\"background: #ece8d9; width: 20%; margin-left: 20px\">\n        <div\n          v-for=\"(item, index) in topKeywords\"\n          :key=\"index\"\n          style=\"margin-top: 15px\"\n        >\n          <div style=\"margin-bottom: 5px; font-weight: 700; font-size: 18px\">\n            {{ item.keyword }}\n          </div>\n          <div style=\"color: grey\">count:{{ item.count }}</div>\n        </div>\n      </el-card>\n    </div>\n  </div>\n</template>\n\n<script>\n// const data = require(\"../assets/data.json\");\nconst data = require(\"../assets/final_output_1107.json\");\n\nexport default {\n  data() {\n    return {\n      pageTitle: this.$route.query.title,\n      tableData: [],\n      currentPage: 1,\n      itemsPerPage: 5,\n      isDown: [false, false, false, false, false],\n      linkList: {},\n      topKeywords: [],\n    };\n  },\n  methods: {\n    goBack() {\n      this.$router.back();\n    },\n    nextPage() {\n      if (this.currentPage < this.totalPages) {\n        this.currentPage++;\n      }\n      this.isDown = [false, false, false, false, false];\n    },\n    prevPage() {\n      if (this.currentPage > 1) {\n        this.currentPage--;\n      }\n      this.isDown = [false, false, false, false, false];\n    },\n    downClick(i) {\n      this.$set(this.isDown, i, !this.isDown[i]);\n    },\n    getDomain(url) {\n      const start = url.indexOf(\"://\") + 3;\n      const end = url.indexOf(\"/\", start);\n      return end === -1 ? url.substring(start) : url.substring(start, end);\n    },\n    filterNewsByKeyword(data, keyword) {\n      const filteredNews = [];\n\n      // 遍历每个大分类\n      for (const category in data) {\n        if (data[category]?.item) {\n          // 遍历每个分类下的新闻数组\n          data[category].item.forEach((newsItem) => {\n            const relevantSources = newsItem.news_sources.filter((source) =>\n              source.keywords.includes(keyword)\n            );\n\n            if (relevantSources.length > 0) {\n              filteredNews.push({\n                topic: newsItem.topic,\n                summary: newsItem.summary,\n                news_sources: newsItem.news_sources,\n              });\n            }\n          });\n        }\n      }\n\n      return filteredNews;\n    },\n  },\n  computed: {\n    totalPages() {\n      return Math.ceil(this.tableData.item.length / this.itemsPerPage);\n    },\n    paginatedData() {\n      const start = (this.currentPage - 1) * this.itemsPerPage;\n      return this.tableData.item.slice(start, start + this.itemsPerPage);\n    },\n  },\n  created() {\n    switch (this.$route.query.title) {\n      case \"Business\":\n        this.tableData = data.business;\n        break;\n      case \"Entertainment\":\n        this.tableData = data.entertainment;\n        break;\n      case \"General\":\n        this.tableData = data.general;\n        break;\n      case \"Health\":\n        this.tableData = data.health;\n        break;\n      case \"Science\":\n        this.tableData = data.science;\n        break;\n      case \"Sports\":\n        this.tableData = data.sports;\n        break;\n      case \"Technology\":\n        this.tableData = data.technology;\n        break;\n      default:\n        const filteredData = this.filterNewsByKeyword(\n          data,\n          this.$route.query.title\n        );\n        this.tableData = filteredData;\n    }\n    console.log(\"tableData\", this.tableData);\n    // 获取所有 news_sources 的关键词\n    const getAllKeywords = (data) => {\n      const allKeywords = [];\n      Object.values(data).forEach((category) => {\n        category.item.forEach((topic) => {\n          topic.news_sources.forEach((news) => {\n            allKeywords.push(...news.keywords);\n          });\n        });\n      });\n      return allKeywords;\n    };\n\n    // 统计关键词频率\n    const countKeywords = (keywords) => {\n      return keywords.reduce((countMap, keyword) => {\n        countMap[keyword] = (countMap[keyword] || 0) + 1;\n        return countMap;\n      }, {});\n    };\n\n    // 筛选出出现次数最多的前十个关键词\n    const getTopKeywords = (keywordCount, topN = 10) => {\n      return Object.entries(keywordCount)\n        .sort((a, b) => b[1] - a[1]) // 按出现次数从高到低排序\n        .slice(0, topN) // 取前 topN 个\n        .map(([keyword, count]) => ({ keyword, count }));\n    };\n\n    // 运行逻辑\n    const allKeywords = getAllKeywords(data);\n    const keywordCount = countKeywords(allKeywords);\n    const topKeywords = getTopKeywords(keywordCount);\n    this.topKeywords = topKeywords;\n    console.log(\"前十个出现次数最多的关键词：\", topKeywords);\n  },\n};\n</script>\n\n<style scoped>\n@font-face {\n  font-family: \"knorke\";\n  src: url(\"../assets/font/Knorke.ttf\");\n}\n.about {\n  background: #494949;\n}\n.title {\n  font-family: \"knorke\";\n  color: #d7d1d1;\n}\n.top {\n}\n.content {\n  padding: 0px 100px 100px 100px;\n  display: flex;\n}\n.content-container {\n  display: flex;\n  flex-direction: column;\n  justify-content: center;\n  align-items: center;\n  height: 200px;\n  background: #f1f1f1;\n  border: 1px solid #ccc;\n  padding: 20px;\n  text-align: center;\n}\n.paginatio